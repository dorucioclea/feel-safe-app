{
  "name": "ionic-blueprint",
  "version": "1.0.0",
  "author": "prototype.berlin",
  "homepage": "https://prototype.berlin",
  "scripts": {
    "ng": "ng",
    "start": "ionic serve -b",
    "start:hacking": "ionic serve -b -- -c=hacking",
    "start:staging": "ionic serve -b -- -c=staging",
    "start:production": "ionic serve -b -- -c=production",
    "emulate:ios:hacking": "ionic cordova emulate ios --c=hacking",
    "emulate:ios:hacking:l": "ionic cordova emulate ios -l --c=hacking",
    "emulate:ios": "ionic cordova emulate ios",
    "emulate:ios:l": "ionic cordova emulate ios -l",
    "build:ios:dev": "ionic cordova build ios",
    "build:ios:dev:prod": "ionic cordova build ios --c=development-prod",
    "build:ios:staging": "ionic cordova build ios --c=staging",
    "build:ios:staging:prod": "ionic cordova build ios --c=staging-prod",
    "build:ios:prod": "ionic cordova build ios --prod",
    "build:android:dev": "ionic cordova build android",
    "build:android:dev:prod": "ionic cordova build android --c=development-prod",
    "build:android:staging": "ionic cordova build android --c=staging",
    "build:android:staging:prod": "ionic cordova build android --c=staging-prod",
    "build:android:production": "ionic cordova build android --prod",
    "build:android:release": "ionic cordova build android --prod --release",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint:staged": "node scripts/lint-staged.js --no-hard-exit",
    "check-dependencies": "node scripts/dependency-checker.js",
    "e2e": "ng e2e"
  },
  "private": true,
